{
    "presets": [
        "es2015",
        "react",
        "stage-2"
    ],
    "env": {
        "server": {
            "plugins": [
                [
                    "css-modules-transform",
                    {
                        "preprocessCss": "./sass-loader.js",
                        "generateScopedName": "[hash:8]",
                        "extensions": [
                            ".scss"
                        ]
                    }
                ]
            ]
        }
    }
}