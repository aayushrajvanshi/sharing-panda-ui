{
  "name": "sharing-panda",
  "version": "1.0.0",
  "description": "Sharing Panda",
  "keywords": [
    "CrowdFunding",
    "Sharing"
  ],
  "license": "ISC",
  "author": "Aayush Rajvanshi",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/AayushRajvanshi/sharing-panda.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "prod": "npm-run-all --parallel prod:*",
    "prod:server": "node build/bundle.js",
    "prod:build-server": "webpack --config webpack.server.js -p",
    "prod:build-client": "webpack --config webpack.client.js -p",
    "start": "npm run prod:server"
  },
